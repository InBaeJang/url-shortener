		"typeorm": "^0.2.32"
	},
	"devDependencies": {
		"@types/express": "^4.17.11",
		"@types/node": "^15.3.0",
		"@types/pg": "^7.14.11",
		"@types/shortid": "^0.0.29",
		"nodemon": "^2.0.7",
